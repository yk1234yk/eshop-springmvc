<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 根元素是mapper，该元素的namespace属性值相当于该mapper的唯一标识 -->	
<mapper namespace="com.xhxy.eshop.mapper.PictureMapper">
	
	<select id="findByProductId" resultType="com.xhxy.eshop.entity.Picture">
		select * from picture where product_id = #{productId}		
	</select>
	
</mapper>